.metoceanview-selector
  width 250px
  input
    width 100%
  ul
    margin-top 0
    padding 5px
    border-left 1px solid black
    border-right 1px solid black
    border-bottom 1px solid black
  li
    list-style-type none
    text-align left
    cursor pointer
  .selected
    background-color #9CF
